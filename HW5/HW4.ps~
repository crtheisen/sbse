(def) K
( Baseline\(self, numRuns\):) p n
(    self.Chaos\(\)) N
(    self.eMax = self.eMin = self.RawEnergy\(\)) N
(    runs = 1) N
(    ) S
(while) K
( runs < numRuns:   ) p n
(      self.Neighbor\(\)) N
(      eNew = self.RawEnergy\(\)) N
(      ) S
(if) K
( eNew > self.eMax: ) p
(#find largest difference               ) c n
(        self.eMax = eNew) p n
(        self.XVarMax = self.XVar) N
(        ) S
(#print self.XVarMax, eNew) c n
(      ) p
